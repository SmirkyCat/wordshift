{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "wordshift-lobbies",
  "main": "lobbies-worker.js",
  "compatibility_date": "2026-02-20",
  "durable_objects": {
    "bindings": [
      {
        "name": "LOBBY_DIRECTORY",
