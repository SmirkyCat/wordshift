      },
      {
        "name": "LOBBY_ROOM",
        "class_name": "LobbyRoomDO"
      }
    ]
  },
  "migrations": [
    {
      "tag": "wordshift-lobbies-v1",
      "new_sqlite_classes": ["LobbyDirectoryDO", "LobbyRoomDO"]
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "wordshift-words",
      "database_id": "c53dfa20-cf45-4ed8-a1e0-17a2bc175fdf"
    }
  ]
}
